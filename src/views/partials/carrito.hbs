<h1>este es el carrito </h1>
<div>

    <table class="table">
  <thead>
    <tr>
      <th scope="col">carrito ID = </th>
      <th scope="col">productos = </th>


    </tr>
  </thead>
  <tbody>

    {{#each carrito}}
        <tr>
            <th scope="row">{{this.id}}</th>
            {{#each this.productos}}
              <td><img width="50" src="{{this.foto}}"  alt="not found"></td>              
            {{/each}}
        </tr>
    {{/each}}
  </tbody>
</table>

<button class="btn btn-primary" onclick="location.href='/enviarMensajes'" >finalizar compra</button>

</div>